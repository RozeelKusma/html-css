{
	"name": "vite-project",
	"private": true,
	"version": "0.0.0",
	"author": {
		"name": "Rojil Kusma",
		"url": "https://github.com/rozeelkusma",
		"email": "kusmarozeel@gmail.com"
	},
	"engines": {
		"node": ">=14",
		"yarn": "Please-use-pnpm",
		"pnpm": ">=7",
		"npm": "Please-use-pnpm"
	},
	"scripts": {
		"commit": "cz",
		"prepare": "husky install",
		"dev": "vite",
		"build": "tsc && vite build",
		"sync:i18": "ts-node --project scripts/tsconfig.json scripts/syncTranslations.ts",
		"i18": "pnpm sync:i18",
		"preview": "vite preview",
		"format": "prettier -uw --ignore-path .gitignore .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx src",
		"run-stylelint": "stylelint --cache --fix --ignore-path .gitignore \"**/*.css\"",
		"lint": "run-p run-tsc run-eslint run-stylelint",
		"mock:server": "json-server --watch ./mock/index.json --config ./mock/config.json",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@ant-design/plots": "^1.2.2",
