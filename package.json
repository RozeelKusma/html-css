		"i18next-http-backend": "^1.4.4",
		"jwt-decode": "^3.1.2",
		"path-to-regexp": "^6.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.38.0",
		"react-i18next": "^12.0.0",
		"react-icons": "^4.6.0",
		"react-perfect-scrollbar": "^1.5.8",
		"react-router-dom": "^6.4.2",
